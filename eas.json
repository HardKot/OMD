{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "demo",
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "APP_VARIANT": "preview"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "main",
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
      },
      "android": {}
    }
  },
  "submit": {
    "production": {}
  }
}
