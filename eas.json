{
	"cli": {
		"version": ">=2.1.0"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"channel": "dev",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
				"APP_VARIANT": "dev"
			}
		},
		"preview-close": {
			"distribution": "internal",
			"channel": "dev",
			"env": {
				"YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
			},
			"android": {
				"buildType": "app-bundle"
			}
		},
		"preview-open": {
			"distribution": "internal",
			"channel": "preview",
			"env": {
				"YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
			},
			"android": {
				"buildType": "app-bundle"
			}
		},
		"production": {
			"channel": "main",
			"distribution": "store",
			"env": {
				"YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
			},
			"android": {}
		}
	},
	"submit": {
		"production": {}
	}
}
