{
	"cli": {
		"version": "^3.3.2"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"android": {
				"buildType": "apk",
				"prebuildCommand": "prebuild"
			},
			"ios": {
				"simulator": true,
				"prebuildCommand": "prebuild"
			},
			"env": {
				"YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
				"APP_VARIANT": "dev"
			}
		},
		"preview-close": {
			"distribution": "internal",
			"channel": "preview",
			"env": {
				"APP_VARIANT": "alpha"
			},
			"android": {
				"buildType": "app-bundle"
			},
			"ios": {
				"prebuildCommand": "prebuild"
			}
		},
		"productionApk": {
			"channel": "production",
			"distribution": "store",
			"env": {
				"APP_VARIANT": "alpha",
				"FLIPPER_DISABLE": "1"
			},
			"android": {
				"buildType": "apk"
			}
		},
		"production": {
			"channel": "production",
			"distribution": "store",
			"env": {
				"APP_VARIANT": "alpha",
				"FLIPPER_DISABLE":  "1"
			},
			"ios": {
				"prebuildCommand": "prebuild"
			}
		}
	},
	"submit": {
		"production": {
			"android": {
				"changesNotSentForReview": true,
				"track": "production"
			},
			"ios": {}
		}
	}
}
